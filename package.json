  "description": "Lovelace custom card for tracking Vehicle status",
  "keywords": [
    "home-assistant",
    "homeassistant",
    "hass",
    "automation",
    "lovelace",
    "custom-cards"
  ],
  "author": "Viet Ngoc",
  "repository": {
    "type": "git",
    "url": "https://github.com/ngocjohn/vehicle-status-card",
    "repo": "ngocjohn/vehicle-status-card"
  },
  "license": "MIT",
  "dependencies": {
    "@mdi/js": "^7.4.47",
    "debug": "^4.4.3",
    "get-promisable-result": "^1.0.1",
    "home-assistant-js-websocket": "^9.5.0",
    "home-assistant-styles-manager": "^3.1.0",
    "lit": "^3.3.1",
    "memoize-one": "^6.0.0",
    "superstruct": "^2.0.2",
    "tinycolor2": "^1.6.0"
  },
  "devDependencies": {
    "@babel/core": "^7.28.4",
    "@babel/plugin-proposal-decorators": "^7.28.0",
    "@babel/preset-env": "^7.28.3",
    "@dotenvx/dotenvx": "^1.51.0",
    "@rollup/plugin-babel": "^6.0.4",
    "@rollup/plugin-commonjs": "^28.0.6",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^16.0.2",
    "@rollup/plugin-replace": "^6.0.2",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^12.1.4",
    "@types/debug": "^4.1.12",
    "@types/leaflet": "^1.9.21",
    "@types/leaflet-providers": "^1.2.5",
    "@types/lodash": "^4.17.20",
    "@types/node": "^24.7.2",
    "@typescript-eslint/eslint-plugin": "^8.46.0",
    "@typescript-eslint/parser": "^8.46.0",
    "@watchable/unpromise": "^1.0.2",
    "axios": "^1.12.2",
    "cross-env": "^10.1.0",
    "es-toolkit": "^1.40.0",
    "eslint": "^9.37.0",
    "eslint-plugin-perfectionist": "^4.15.1",
    "eslint-plugin-unused-imports": "^4.2.0",
    "husky": "^9.1.7",
    "js-yaml": "^4.1.0",
    "leaflet": "^1.9.4",
    "leaflet-providers": "^2.0.0",
    "lint-staged": "^16.2.4",
    "postcss-preset-env": "^10.4.0",
    "rollup": "^4.52.4",
    "rollup-plugin-postcss": "^4.0.2",
    "rollup-plugin-postcss-lit": "^2.2.0",
    "rollup-plugin-serve": "1.1.1",
    "swiper": "^12.0.2",
    "tslib": "^2.8.1",
    "typescript": "^5.9.3"
  },
  "scripts": {
    "start": "rollup -c rollup.config.js --bundleConfigAsCjs --watch",
    "lint": "eslint src/**/*.ts",
    "rollup": "rollup -c rollup.config.js --bundleConfigAsCjs",
    "build": "pnpm run lint && pnpm run rollup",
    "dev": "pnpm run start",
    "dev-serve": "cross-env PORT=8222 pnpm start",
    "debug": "cross-env DEBUG=true pnpm run dev",
    "prepare": "husky",
    "clean": "rm -rf dist build && mkdir -p dist build",
    "clean_cache": "pnpm run clean_delfiles && pnpm run clean_deldirs",
    "clean_delfiles": "rm -rf node_modules/.cache/rollup-plugin-typescript2/*",
    "clean_deldirs": "rm -rf node_modules/.cache/rollup-plugin-typescript2/rpt2_*"
  }
}